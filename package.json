{
  "name": "interkm",
  "version": "0.0.1",
  "description": "Client application for read/write rw1990 keys",
  "main": "main.js",
  "scripts": {
    "serve": "electron .",
    "start": "cross-env NODE_ENV=development concurrently \"npm run serve\" \"webpack --config webpack.config.dev.js --colors\"",
    "build": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config webpack.config.prod.js --colors",
    "build-windows": "npm run build && build --win --x64",
    "build-linux": "npm run build && build --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "interkm",
    "appId": "io.github.yokitoki",
    "files": [
      "app/build/**/*",
      "app/src/assets/**/*",
      "app/index.html",
      "node_modules/**/*",
      "main.js",
      "package.json"
    ],
    "win": {
      "target": [
        "portable"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "tar.gz"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "author": {
    "name": "Stanislav Tamat",
    "email": "libastral.so@yandex.ru"
  },
  "license": "MIT",
  "homepage": "https://yokitoki.github.io/interkm",
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "serialport": "^6.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-reload": "^1.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "webpack": "^3.10.0"
  },
  "devEngines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0",
    "yarn": ">=1.7.0"
  }
}
